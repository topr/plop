wget -P ~/Downloads/ --no-check-certificate --no-cookies --header "Cookie: oraclelicense=accept-securebackup-cookie" http://download.oracle.com/otn-pub/java/jdk/7u79-b15/jdk-7u79-linux-x64.tar.gz

wget -P ~/Downloads/ --no-check-certificate --no-cookies --header "Cookie: oraclelicense=accept-securebackup-cookie" http://download.oracle.com/otn-pub/java/jdk/8u77-b03/jdk-8u77-linux-x64.tar.gz

mkdir -p ~/devel/soft/jdk

tar xzf ~/Downloads/jdk-7u79-linux-x64.tar.gz -C ~/devel/soft/jdk/
